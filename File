	var voice4 = "\n\n----------------------------------------------------------------------\n"
	var voice5 = "\nIssue: "



	
	document.forms[1].sepcasenote.value = call+document.forms[0].the_time.value+'\n'+voice1+document.forms[0].phoneno.value+voice5+document.forms[0].sepissue.value+voice2+document.forms[0].septrouble.value+voice3+document.forms[0].resummary.value+voice4;
}

